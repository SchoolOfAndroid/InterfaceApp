<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.sumod.interfaceapp"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity_"></activity>

        <activity android:name=".SignUpActivity_"></activity>

        <activity android:name=".PostJobActivity_"></activity>

        <activity android:name=".FindJobActivity_"></activity>

        <activity android:name=".JobListActivity_"></activity>

        <activity android:name=".EntryActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".LoginActivity_"></activity>

        <activity android:name=".multisms.ContactSelectionActivity"></activity>

        <activity android:name=".multisms.MessageEditorActivity"></activity>

    </application>

</manifest>
